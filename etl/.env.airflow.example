AIRFLOW_UID=50000
AIRFLOW_GID=0
AIRFLOW__CORE__LOAD_EXAMPLES=False
AIRFLOW_ADMIN_USER=admin
AIRFLOW_ADMIN_PASSWORD=admin
AIRFLOW_ADMIN_EMAIL=admin@example.com

# SQLAlchemy DSN (DAGs normalize it for psycopg)
SRTA_DB_URI=postgresql+psycopg://srta:srta_pw@postgres:5432/srta

# ---- FILL LOCALLY in etl/.env.airflow (DO NOT COMMIT REAL VALUES) ----
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1

# S3 target (safe to keep as-is)
S3_BUCKET=srta-demo-bucket-fl
S3_PREFIX=exports/trades

# For LocalStack (containers use localstack:4566; host CLI uses localhost:4566)
# AWS_ENDPOINT_URL=http://localstack:4566
